{
  "name": "backend",
  "version": "1.0.0",
  "description": "PMT backend",
  "main": "src/server.js",
  "scripts": {
    "build": "cd client && yarn run build",
    "install-client": "cd client && yarn install",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --runInBand --forceExit",
    "heroku-postbuild": "yarn run install-client && yarn run build",
    "server": "nodemon server.js",
    "develop": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run start --prefix client\"",
    "start": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run start --prefix client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jest": "26.6.0",
    "mongoose": "^6.3.3",
    "nodemon": "^2.0.16",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "supertest": "^6.2.3"
  },
  "devDependencies": {},
  "keywords": []
}
